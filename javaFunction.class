public class MathOperation {
  public static void main(String[] args) {
      
    int number1 = 25;
    int number2 = 5;
    int number3 = 5;

    if (number1 + number2 == number3 && number1 - number2 == number3) {
      System.out.println(" +  - ");
    }
    else if (number1 + number2 == number3 && number1 * number2 == number3) {
      System.out.println(" +  * ");
    }
    else if (number1 + number2 == number3 && number1 / number2 == number3) {
      System.out.println(" +  / ");
    }
    else if (number1 - number2 == number3 && number1 * number2 == number3) {
      System.out.println(" -  * ");
    }
    else if (number1 - number2 == number3 && number1 / number2 == number3) {
      System.out.println(" -  / ");
    }
    else if (number1 * number2 == number3 && number1 / number2 == number3) {
      System.out.println(" *  / ");
    }

    else if (number1 + number2 == number3) {
      System.out.println("+");
    }
    else if (number1 - number2 == number3) {
      System.out.println(" - ") ;
    }
    else if (number1 * number2 == number3) {
      System.out.println(" * ");
    }
    else if (number1 / number2 == number3) {
      System.out.println(" / ");
    }
    else {
      System.out.println("None");
    }
  }
}
